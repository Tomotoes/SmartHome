{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "node:start",
			"runtimeExecutable": "nodemon",
			"program": "${workspaceFolder}/server/index.js",
			"restart": true,
			"runtimeArgs": ["--nolazy"],
			"preLaunchTask": "",
			"env": {
				"NODE_ENV": "development"
			}
		},
		{
			"name": "node:attach",
			"type": "node",
			"request": "attach",
			"address": "localhost",
			"port": 5858,
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "vue: chrome",
			"url": "http://localhost:8080",
			"webRoot": "${workspaceFolder}/src",
			"breakOnLoad": true,
			"sourceMapPathOverrides": {
					"webpack:///src/*": "${webRoot}/*"
			}
		}
	],
	"compounds": []
}
